generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        String   @id @default(uuid())
  nome      String
  telefone  String   @unique
  papel     Papel
  createdAt DateTime @default(now())
}

enum Papel {
  MORADOR
  COLETOR
  ADMIN
}